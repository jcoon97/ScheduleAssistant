{
  "name": "coverage",
  "description": "Handle coverage requests for 2U Learning Assistants",
  "license": "MIT",
  "main": "build/server.js",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf build dist",
    "build": "tsc --project tsconfig.json",
    "cleanbuild": "pnpm run clean && pnpm run build",
    "start": "pnpm run cleanbuild && node build/server.js",
    "start:dev": "tsc --project tsconfig.json --watch && NODE_ENV=development nodemon build",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^89.0.0",
    "jsonwebtoken": "^8.5.1",
    "tslog": "^3.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
