{
  "name": "@coverage/server",
  "private": true,
  "main": "build/startup.js",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf build dist",
    "build": "tsc -p tsconfig.json",
    "cleanbuild": "pnpm run clean && pnpm run build",
    "preinstall": "npx only-allow pnpm",
    "start": "cross-env NODE_ENV=production node build/startup.js",
    "start:dev": "cross-env NODE_ENV=development pnpm run watch",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests --verbose",
    "watch": "tsc-watch --noClear -p tsconfig.json --onSuccess \"node build/startup.js\""
  },
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.24.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "googleapis": "^89.0.0",
    "graphql": "15.7.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "tslog": "^3.2.2",
    "type-graphql": "^1.2.0-rc.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.39",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@types/express-jwt": "^6.0.2",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.7",
    "cross-env": "^7.0.3",
    "faker": "^5.5.3",
    "gaxios": "^4.3.2",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  }
}
